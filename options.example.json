{
    "urban": {
        "description": "Is the scene urban?",
        "type": "boolean",
        "required": true,
        "keybinds": {
            "1": true,
            "2": false
        }
    },
    "flooding": {
        "description": "Is there flooding?",
        "type": "boolean",
        "required": true
    },
    "depth": {
        "description": "Estimate the flood depth:",
        "type": "select-one",
        "options": {
            "low": "knee-deep or lower",
            "medium": "between knee-deep and chest-deep",
            "high": "above chest-deep"
        },
        "disabled": {
            "flooding": false
        },
        "keybinds": {
            "l": "low",
            "m": "medium",
            "h": "high"
        }
    },
    "objects": {
        "description": "Select objects:",
        "type": "select-many",
        "options": {
            "buildings": "houses, businesses, roofs, and other structures",
            "people": "men, women, and children",
            "street": "fire hydrants, street signs, curbs, traffic lights, parking meters, traffic cones, street lights, railings, etc.",
            "vegetation": "trees, shrubs, bushes, etc.",
            "vehicles": "cars, trucks, motorcycles, buses, etc."
        }
    }
}