{
    "urban": {
        "description": "Is the scene urban?",
        "type": "boolean",
        "required": true,
        "keybinds": {
            "1": true,
            "2": false
        }
    },
    "flooding": {
        "description": "Is there flooding?",
        "type": "boolean",
        "required": true
    },
    "depth": {
        "description": "Estimate the flood depth:",
        "type": "select-one",
        "options": [
            "low",
            "medium",
            "high"
        ],
        "disabled": {
            "flooding": false
        },
        "keybinds": {
            "l": "low",
            "m": "medium",
            "h": "high"
        }
    },
    "objects": {
        "description": "Select objects:",
        "type": "select-many",
        "options": [
            "buildings",
            "people",
            "street",
            "vegetation",
            "vehicles"
        ]
    }
}